<div id="LogoBox" class="white">
	<p><%= image_tag("medadata_logo.jpg") %></p>  
</div>


<div class="login">
	
	<div class="flash_notice" id="Flash">Please log in first and then we'll send you right along.</div>
		
	<div id="login_dialog" class="login_dialog">
	
		<% form_for @user_session do |f| %>
  			<%= f.error_messages %>
  			<div id ="user_name_login">
   				<h2><%= f.label :username %></h2>
    			<%= f.text_field :username %>
  				<h2><%= f.label :password %></h2>
    			<%= f.password_field :password %><br />
      			<label><input type="checkbox" name="remember_me" class="auto" /> Remember me on this computer</label><br />
  				<%= f.submit "Sign In" %>
			</div>
		<% end %>
	</div>
</div>






    
    
    
    

    


