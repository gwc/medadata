<h1>Editing provider_role</h1>

<% form_for(@provider_role) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :role_id %><br />
    <%= f.text_field :role_id %>
  </p>
  <p>
    <%= f.label :provider_id %><br />
